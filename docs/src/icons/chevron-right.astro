---
import type { IconProps } from "./utils";

export type Props = IconProps;
const { size = 24, ...rest } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  aria-hidden="true"
  {...rest}
>
  <polyline points="9 18 15 12 9 6"></polyline>
</svg>
