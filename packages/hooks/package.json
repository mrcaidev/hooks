  "description": "React utility hooks",
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "homepage": "https://hooks.mrcai.dev",
  "bugs": "https://github.com/mrcaidev/hooks/issues",
  "license": "MIT",
