    "@types/react": "^18.0.21",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "react": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
