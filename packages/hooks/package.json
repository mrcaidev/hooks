{
  "name": "@mrcaidev/hooks",
  "version": "0.5.0",
  "description": "React utility hooks",
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "homepage": "https://github.com/mrcaidev/hooks",
  "bugs": "https://github.com/mrcaidev/hooks/issues",
  "license": "MIT",
  "author": "Yuwang Cai <mrcaidev@gmail.com> (https://mrcai.space)",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrcaidev/hooks"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/hooks.d.ts",
  "scripts": {
    "build": "run-s build:package build:api",
    "build:package": "run-p build:esm build:cjs",
    "build:esm": "tsc --project tsconfig.build.json",
    "build:cjs": "tsc --project tsconfig.node.json",
    "build:api": "mkdir -p etc && api-extractor run --local",
    "clean": "rm -rf dist temp *.tsbuildinfo",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.31.2",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.3"
  }
}
